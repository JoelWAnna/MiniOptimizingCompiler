void mini_main() {
  int three = 3;
  int four  = 4;
  print fib(three + four);
  int x = square(three);
  print x+1+x;
  print fact(three);
  print tailfact(1, four);
}

int fib(int n) {
  if (n<2) {
    return n;
  } else {
    return fib(n-1) + fib(n-2);
  }
}

int square(int x) {
  return x*x;
}

int fact(int n) {
  if (n==0)
      return 1;
  else
      return n * fact(n-1);
}

int tailfact(int a, int n) {
  if (n==0)
      return a;
  else
      return tailfact(a*n, n-1);
}
